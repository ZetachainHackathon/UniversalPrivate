{
  "name": "@repo/sdk",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./web": {
      "types": "./dist/adapters/web/index.d.ts",
      "import": "./dist/adapters/web/index.mjs",
      "require": "./dist/adapters/web/index.js"
    },
    "./node": {
      "types": "./dist/adapters/node/index.d.ts",
      "import": "./dist/adapters/node/index.mjs",
      "require": "./dist/adapters/node/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@railgun-community/engine": "^9.5.1",
    "@railgun-community/shared-models": "7.6.1",
    "@railgun-community/wallet": "10.4.0",
    "brotli-wasm": "^3.0.1",
    "ethers": "6.13.1",
    "level-js": "6.1.0",
    "leveldown": "^6.1.1",
    "snarkjs": "0.7.5"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/leveldown": "^4.0.3",
    "@types/node": "^22.15.3",
    "@types/snarkjs": "0.7.9",
    "tsup": "^8.5.1",
    "typescript": "5.9.2"
  }
}
