{
  "name": "frontend",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "postinstall": "patch-package",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "next": "^16.0.7",
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "turbo": "^2.6.3",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "@railgun-community/shared-models": "7.6.1",
      "@railgun-community/wallet": "10.4.0",
      "ethers": "6.13.1"
    },
    "patchedDependencies": {
      "@railgun-community/shared-models@7.6.1": "patches/@railgun-community__shared-models@7.6.1.patch"
    }
  }
}